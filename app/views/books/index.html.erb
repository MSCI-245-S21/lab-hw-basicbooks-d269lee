
<h1>Books</h1>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>author</th>
      <th>rating</th>
      <th colspan=3></th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      
      <tr>
           <td><%= link_to book.title, book_path(book) %></td>
          
           <td><%= book.author %></td>
           <td><%= book.rating %></td>
         
         
      </tr>
      
    <% end %>
      
      
    
         
  </tbody>
    
</table>

      <p><td><%= link_to "Home", root_path %></td></p>
      <p><td><%= link_to "Add a Book", new_book_path %></td></p>